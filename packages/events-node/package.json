{
  "name": "@dotdo/events-node",
  "version": "0.1.0",
  "description": "Node.js SDK for events.do - Event tracking client with batching, retry, and backpressure handling",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "events.do",
    "analytics",
    "event-tracking",
    "nodejs",
    "sdk",
    "cloudflare",
    "workers"
  ],
  "author": {
    "name": "Platform.do",
    "email": "npm@platform.do",
    "url": "https://platform.do"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dot-do/events.git",
    "directory": "packages/events-node"
  },
  "homepage": "https://events.do",
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
